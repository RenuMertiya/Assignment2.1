import java.util.*;


class test
{
public static void main(String[] args)
{

       Scanner sc=new Scanner(System.in);
       System.out.println("enter number");
       int num1=sc.nextInt();
       
       for(int i=1;i<=10;i++)
       {
              int num=num1*i;
              
              System.out.println(num1+"*"+i+"="+num);
              
              
              
       }
       

}

} 
